{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Redis": "redis:6379",
    "RabbitMQ": "amqp://guest:guest@rabbitmq:5672/"
  },
  "PipelineMonitoring": {
    "CheckIntervalSeconds": 30,
    "HeartbeatTimeoutSeconds": 300,
    "EnableAutoRetry": false,
    "MaxRetryAttempts": 3
  }
}
