{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Error"
    }
  },
  "AllowedHosts": "*",
  "PipelineMonitoring": {
    "CheckIntervalSeconds": 30,
    "HeartbeatTimeoutSeconds": 300,
    "EnableAutoRetry": false,
    "MaxRetryAttempts": 3
  }
}
